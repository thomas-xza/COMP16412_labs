public class Exercise2_2 {
    public static void main(String args[]) {
        for ( int k = 0 ; k < 5 ; k++ ) {

	    for ( int i = 9 ; i >= 0 ; i-- ) {

		for ( int j = 0 ; j < 3 ; j++ ) {

		    String i_str = Integer.toString(i);

		    System.out.print(i_str);

		}
	    
	    }

	    System.out.println();

	}
    }

}
